{
    "name": "lychao8/Laf",
    "description": "The Laf Framework.",
    "homepage": "http://i-vip.cn",
    "keywords": [
        "framework",
        "Laf"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.9",
        "guzzle/guzzle": "~3.9"
    },
    "require-dev": {
        "vinkla/hashids": "^2.0",
        "jenssegers/agent": "^2.2"
    },
    "autoload": {
        "classmap": [
            "Library/"
        ],
        "psr-0": {
            "BC": ""
        },
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {

    },
    "config": {
        "preferred-install": "dist"
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    }
}
